[% INCLUDE metaconfig/httpd/config/system.tt %]
[%- to_process = ['includes', 'handler', 'log', 'lang', 'icon', 'outputfilter', 'browsermatch'] -%] 
[%- FOREACH p IN  to_process -%]
[%-      IF ${p}.defined -%] 
[%-         template = "metaconfig/httpd/config/${p}.tt" -%]
[%          INCLUDE $template desc=$p %]
[%      END -%]
[%- END %]
