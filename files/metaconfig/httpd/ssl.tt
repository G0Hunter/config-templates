[% IF desc.requiressl %]
SSLRequireSSL
[% END -%]
[% IF desc.exists('active') %]
SSLEngine [% desc.active ? "on" : "off" %]
[% END -%]
[% IF desc.exists('ciphersuite') %]
SSLCipherSuite [% desc.ciphersuite.join("!") %]
[% END -%]
[% IF desc.exists('verify_client') %]
SSLVerifyClient [% desc.veryfy_client %]
[% END -%]
[% IF desc.exists('certificate') %]
SSLCertificateFile [% desc.certificate %]
SSLCertificateKeyFile [% desc.key %]
SSLCACertificateFile [% desc.ca %]
[% END -%]
[% IF desc.exists('options') %]
SSLOptions [% desc.options.join(' ') %]
[% END -%]
[% IF desc.exists('require') %]
SSLRequire [% desc.require %]
[% END -%]