[% FOREACH h IN px.set_header.pairs %]
proxy_set_header [% h.key %] [% h.value %];
[% END %]
[% IF px.exists("redirect") %]
proxy_redirect [% px.redirect %];
[% END %]
proxy_pass [% px.pass %];
proxy_cache [% px.exists("proxy_cache") ? px.proxy_cache : "off" %];
[% FOREACH v IN px.valid %]
proxy_cache_valid [% px.valid.codes.join(" ") %] [% px.valid.period %];
[% END %]
