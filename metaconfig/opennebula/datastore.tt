[%- lists = ['bridge_list', 'ceph_host'] -%]
[%- booleans = ['datastore_capacity_check'] -%]
[%- FOR pair IN datastore_ceph.pairs -%]
[%    pair.key.upper %] =
[%-        SWITCH pair.key -%]
[%             CASE lists -%] 		  
 "[%            pair.value.join(' ') %]"
[%             CASE booleans -%] 		  
 "[%            pair.value ? "yes" : "no" %]"
[%             CASE -%]
 "[%            pair.value %]"
[%         END -%]
QUATTOR = 1
[%- END -%]
